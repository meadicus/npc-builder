<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:uk.co.meadicus.npcbuilder.client.attack">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:FlowPanel>
			<g:InlineLabel>Attack Name:</g:InlineLabel>
			<g:TextBox ui:field="attackName"/>
			<g:ListBox ui:field="attackType"/>
			<g:ListBox ui:field="attackGrade"/>
		</g:FlowPanel>
		<g:HTML>
			<hr/>
		</g:HTML>
		<g:FlowPanel>
			<g:InlineLabel>damage type:</g:InlineLabel>
			<g:ListBox ui:field="damageType"/>
			<g:InlineLabel>aligned:</g:InlineLabel>
			<g:TextBox ui:field="aligned"/>			
		</g:FlowPanel>
		<g:FlowPanel>
			<g:InlineLabel>diseased:</g:InlineLabel>
			<g:TextBox ui:field="diseased"/>
			<g:InlineLabel>venomous:</g:InlineLabel>
			<g:TextBox ui:field="venomous"/>
		</g:FlowPanel>
		<g:FlowPanel>
			<g:InlineLabel>armour peircing:</g:InlineLabel>
			<g:ListBox ui:field="armourPeircing"/>
			<g:InlineLabel>keen:</g:InlineLabel>
			<g:ListBox ui:field="keen"/>
		</g:FlowPanel>
		<g:FlowPanel>
			<g:CheckBox ui:field="bleed">bleed</g:CheckBox>
			<a:AreaWidget ui:field="area"/>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 